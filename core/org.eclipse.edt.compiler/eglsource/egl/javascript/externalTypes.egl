package egl.javascript;

ExternalType Job type JavaScriptObject {
		relativePath = "egl/javascript",
		javaScriptName = "Job",
		eglIsSystemAnnotation = yes
	}
	
	name String { @JavaScriptProperty { getMethod = "getName", setMethod = "setName" } };
	runFunction RunFunction { @JavaScriptProperty { getMethod = "getRunFunction", setMethod = "setRunFunction" } };

	function schedule();
	function schedule(duration int in);
	function repeat(duration int in);
	function cancel();
	
end

Delegate RunFunction() {eglIsSystemPart = yes}
end
